{
	"name": "BorrowSanitizer",
	"build": {
		"dockerfile": "Dockerfile"
	},
	"remoteEnv": {
		"PATH": "${containerEnv:PATH}:${containerWorkspaceFolder}"
	},
	"features": {
		"ghcr.io/devcontainers/features/github-cli:1": {
			"version": "latest"
		}
	},
	"customizations": {
		"vscode": {
			"settings": {},
			"extensions": [
				"rust-lang.rust-analyzer"
			]
		}
	}
} 